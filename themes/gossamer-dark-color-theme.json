	"tokenColors": [
		{
			"scope": [
				// "meta.object-literal.key.tsx",
				"meta.template.expression.tsx",
				"meta.template.expression.ts",
				"string.template.js meta.template.expression.js meta.embedded.line.js"
			],
			"settings": {
				"foreground": "#b3b3b3"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#ffffff70"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#A3CE9E"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation.definition"],
			"settings": {
				"foreground": "#5FB3B3"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FAB763"
			}
		},
		{
			"name": "Built-in constant",
			"scope": ["constant.language", "variable.member"],
			"settings": {
				"foreground": "#EE6A6F"
			}
		},
		{
			"name": "User-defined constant",
			"scope": ["constant.character", "constant.other"],
			"settings": {
				"foreground": "#c594c5"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "keyword.operator.word", "keyword.control", "keyword.operator.new.js"],
			"settings": {
				"foreground": "#c594c5"
			}
		},
		// {
