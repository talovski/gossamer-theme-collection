	"tokenColors": [
		{
			// `variable in template strings`
			"scope": "variable.other.readwrite",
			"settings": {
				"foreground": "#A8ABAF"
			}
		},
		{
			"scope": ["string", "punctuation.definition.string"],
			"settings": {
				"foreground": "#8EB673"
			}
		},
		{
			"scope": ["keyword", "storage.type", "punctuation.definition.tag", "keyword.operator", "support.class.component"],
			"settings": {
				"foreground": "#BC70D2"
			}
		},
		{
			"scope": ["punctuation"],
			"settings": {
				"foreground": "#818387"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#33a2f2"
			}
		},
		{
			"scope": ["meta.object-literal.key", "variable.other.property"],
			"settings": {
				"foreground": "#51a8b3"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#de5d68"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#dbb671"
			}
		},
		{
			"scope": ["support.type.primitive", "entity.name.type", "entity.other.inherited-class", "constant.numeric"],
			"settings": {
				"foreground": "#e2c792"
			}
		},
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#5a5b5e"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#51a8b3"
			}
		}
	]
