  "tokenColors": [
    {
      "name": "String template variables",
      "scope": ["meta.template.expression", "string.template.js meta.template.expression.js meta.embedded.line.js"],
      "settings": {
        "foreground": "#B3B3B3"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#ffffff70"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#A3CE9E"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.definition"],
      "settings": {
        "foreground": "#B3B3B3"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FAB763"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#FAB763"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#c594c5"
      }
    },
    {
      "name": "Member Variable",
      "scope": "variable.member",
      "settings": {
        "foreground": "#EE6A6F"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.word",
        "keyword.control",
        "keyword.operator.new.js",
        "storage.type",
        "storage.modifier",
        "keyword.operator"
      ],
      "settings": {
        // "foreground": "#DE7272"
        "foreground": "#C695C6"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.separator", "punctuation.terminator", "punctuation.accessor"],
      "settings": {
        "foreground": "#ffffff70"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.section",
      "settings": {
        "foreground": "#ffffffdd"
      }
    },
    {
      "name": "Annotation Punctuation",
      "scope": "punctuation.definition.annotation",
      "settings": {
        "foreground": "#B3B3B3"
      }
    },
    {
      "scope": "punctuation.section",
      "settings": {
        "foreground": "#B3B3B3"
      }
    },
    {
      "name": "JavaScript Dollar",
      "scope": [
        "variable.other.dollar.only.js",
        "variable.other.object.dollar.only.js",
        "variable.type.dollar.only.js",
        "support.class.dollar.only.js"
      ],
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#b3b3b3"
      }
    },
    {
      "name": "Entity name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "Entity name",
      "scope": "entity.name",
      "settings": {
        "foreground": "#E9A652"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        // "fontStyle": "underline",
        "foreground": "#6699cc"
      }
    },
    {
      "name": "Function argument",
