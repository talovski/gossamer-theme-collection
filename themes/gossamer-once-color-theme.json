    "tokenColors": [
        {
            "scope": "string",
            "settings": {
                "foreground": "#A1C181"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#5E636E"
            }
        },
        {
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#b477cfff"
            }
        },
        {
            "scope": [
                "meta.function",
                "meta.function-call",
                "entity.name.tag",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#73ADE9FF"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "meta.tag.attributes"
            ],
            "settings": {
                "foreground": "#7EB2BD"
            }
        },
        {
            "scope": [
                "variable",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#d07277ff"
            }
        },
        {
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#ADB2BD"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "entity.other.attribute-name.class",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#dfc184"
            }
        },
        {
            "scope": [
                "punctionation",
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.definition.string.end",
                "punctuation.definition.string.begin"
            ],
            "settings": {
                "foreground": "#b477cfff"
            }
        }
    ]
